{
  "plugins": ["prettier-plugin-tailwindcss"],
  "pluginSearchDirs": false,
  "arrowParens": "always",
  "bracketSpacing": true,
  "embeddedLanguageFormatting": "auto",
  "htmlWhitespaceSensitivity": "css",
  "printWidth": 80,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "semi": true,
  "tabWidth": 4,
  "trailingComma": "es5"
}
